{
  "mcpServers": {
    "mcp-server-code-runner": {
      "command": "npx",
      "args": [
        "-y",
        "@iflow-mcp/mcp-server-code-runner"
      ]
    },
    "mcp-taskmanager": {
      "command": "npx",
      "args": [
        "-y",
        "@iflow-mcp/mcp-taskmanager@1.0.2"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@iflow-mcp/playwright-mcp"
      ]
    },
    "spec-kit": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-spec-driven-development@latest"
      ]
    }
  },
  "outputStyle": "CodeFlow Expert"
}